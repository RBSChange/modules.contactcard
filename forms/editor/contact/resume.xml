<?xml version="1.0"?>
<panel>
	<section name="properties" linkedtab="properties" labeli18n="modules.uixul.bo.doceditor.section.Properties" image="{HttpHost}/icons/small/edit-properties.png">
		<property name="label" labeli18n="modules.uixul.bo.doceditor.property.Label" />
		<property name="emails" labeli18n="modules.contactcard.document.contact.Mails" />
		<property name="author" labeli18n="modules.uixul.bo.doceditor.property.Author" />
		<property name="creationdate" labeli18n="modules.uixul.bo.doceditor.property.Creationdate" />
	</section>
	<section name="content" labeli18n="modules.contactcard.document.documentcard.Photo" image="{HttpHost}/icons/small/users.png">
		<property name="photo" labeli18n="modules.contactcard.document.documentcard.Photo" />
	</section>

	<section name="localization" linkedtab="localization" labeli18n="modules.uixul.bo.doceditor.section.Localization" image="{HttpHost}/icons/small/translate.png">
