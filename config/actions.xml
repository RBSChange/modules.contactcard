<?xml version="1.0"?>
<actions extends="generic">
	<action name="createContact">
		<parameter name="listWidget" />
		<body><![CDATA[
    		this.createDocumentEditor('modules_contactcard_contact', listWidget.getSelectedItems()[0].id);
    	]]></body>
	</action>
	<action name="createFolder">
		<parameter name="listWidget" />
		<body><![CDATA[
			this.createDocumentEditor('modules_contactcard_folder', listWidget.getSelectedItems()[0].id);
		]]></body>
	</action>
</actions>
